<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/lp_data_api/db" VERSION="20120822" COMMENT="XMLDB file for Moodle local/lp_data_api"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="lp_datamap_hierarchy" COMMENT="stores full length copy of the hierarchy">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="auth"/>
        <FIELD NAME="auth" TYPE="char" LENGTH="30" NOTNULL="false" SEQUENCE="false" COMMENT="Authentication type for this string" PREVIOUS="id" NEXT="hierarchy_in"/>
        <FIELD NAME="hierarchy_in" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" COMMENT="Input from the SSO for the hierarchy, to be stored for later use" PREVIOUS="auth"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>