<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/lp_course_progress/db" VERSION="20120308" COMMENT="XMLDB file for Moodle blocks/lp_course_progress"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_lp_course_progress" COMMENT="Default comment for block_lp_course_progress, please edit me" NEXT="lp_incomplete_notification">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="lp_incomplete_notification" COMMENT="records details of scheduled incomplete notification emails" PREVIOUS="block_lp_course_progress">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="description"/>
        <FIELD NAME="description" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="id" NEXT="startdate"/>
        <FIELD NAME="startdate" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="description" NEXT="enddate"/>
        <FIELD NAME="enddate" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="startdate" NEXT="courses"/>
        <FIELD NAME="courses" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" PREVIOUS="enddate" NEXT="message"/>
        <FIELD NAME="message" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" PREVIOUS="courses" NEXT="frequency"/>
        <FIELD NAME="frequency" TYPE="char" LENGTH="10" NOTNULL="false" SEQUENCE="false" PREVIOUS="message" NEXT="next"/>
        <FIELD NAME="next" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="frequency" NEXT="type"/>
        <FIELD NAME="type" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" COMMENT="legacy - need to discover its purpose" PREVIOUS="next" NEXT="status"/>
        <FIELD NAME="status" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="type"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>